{
  "name": "@codex-infinity/codex-infinity",
  "version": "1.2.0",
  "license": "Apache-2.0",
  "description": "Codex Infinity - a smarter coding agent that can run forever",
  "bin": {
    "codex-infinity": "bin/codex-infinity.js"
  },
  "scripts": {
    "prepack": "node -e \"process.env.CODEX_INFINITY_REQUIRED_GROUPS='any';require('child_process').execFileSync(process.execPath,['scripts/verify_vendor.js'],{stdio:'inherit'})\""
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "os": [
    "linux",
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "files": [
    "bin",
    "vendor/x86_64-unknown-linux-gnu",
    "vendor/x86_64-pc-windows-gnu",
    "vendor/x86_64-pc-windows-msvc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lee101/codex-infinity.git",
    "directory": "codex-cli"
  },
  "keywords": [
    "ai",
    "coding",
    "agent",
    "cli",
    "autonomous",
    "codex"
  ]
}
